    "@solana/web3.js": "^1.24.1",
    "@types/bs58": "^4.0.1",
    "bn.js": "^5.2.0",
    "borsh": "^0.4.0",
    "bs58": "^4.0.1",
    "crypto-js": "^4.1.1",
